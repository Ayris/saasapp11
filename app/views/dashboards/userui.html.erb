<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h1 class="text-center">Upload your file</h1>
    <p class="text-center">Reach out if you have any questions or comments.</p>
    <div class="well">
        <%= form_tag import_file_path, multipart: true do %>
            <%= file_field_tag :file %>
            <%= submit_tag "Import" %>
        <% end %>
    </div>
  </div>
</div>